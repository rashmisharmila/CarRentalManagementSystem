            <Button fx:id="btnlogin" layoutX="198.0" layoutY="324.0" mnemonicParsing="false" onAction="#loginOnAction" prefHeight="27.0" prefWidth="98.0" style="-fx-background-color: #50b1b0;" text="LOGIN" />
